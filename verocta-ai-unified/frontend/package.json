{
  "name": "verocta-frontend",
  "version": "1.0.0",
  "type": "module",
  "description": "VeroctaAI Financial Intelligence Platform - Frontend",
  "main": "index.js",
  "scripts": {
    "dev": "vite --host 0.0.0.0 --port 3000",
    "serve": "vite preview --host 0.0.0.0 --port 3000",
    "build": "vite build --mode production",
    "build:dev": "vite build --mode development",
    "build:vercel": "vite build --mode production && echo 'Vercel build complete'",
    "preview": "vite preview",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx,css,md}",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "analyze": "vite-bundle-analyzer dist/stats.html",
    "vercel-build": "npm run build:vercel"
  },
  "keywords": [
    "veroctaai",
    "frontend",
    "react",
    "ai"
  ],
  "author": "VeroctaAI Team",
  "license": "MIT",
  "dependencies": {
    "@headlessui/react": "^1.7.0",
    "@heroicons/react": "^2.0.0",
    "@hookform/resolvers": "^5.2.1",
    "@types/jwt-decode": "^2.2.1",
    "axios": "^1.3.0",
    "chart.js": "^4.5.0",
    "jwt-decode": "^4.0.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.62.0",
    "react-router-dom": "^6.8.0",
    "tailwindcss": "^3.2.0",
    "yup": "^1.7.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^3.1.0",
    "autoprefixer": "^10.4.0",
    "eslint": "^8.34.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react": "^7.32.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.0",
    "prettier": "^2.8.0",
    "typescript": "^4.9.0",
    "vite": "^4.1.0",
    "vitest": "^0.29.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}
