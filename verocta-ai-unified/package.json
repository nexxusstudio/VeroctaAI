{
  "name": "verocta-unified",
  "version": "2.0.0",
  "description": "VeroctaAI AI-Powered Financial Intelligence Platform - Unified Backend & Frontend",
  "main": "backend/app.py",
  "scripts": {
    "dev": "concurrently \"npm run dev:frontend\" \"npm run dev:backend\"",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:backend": "cd backend && python app.py",
    "dev:vercel": "vercel dev",
    "build": "npm run build:frontend",
    "build:frontend": "cd frontend && npm run build",
    "build:vercel": "cd frontend && npm run build:vercel",
    "start": "cd backend && python app.py",
    "install:all": "npm install && cd frontend && npm install && cd ../backend && pip install -r requirements.txt",
    "install:vercel": "npm install && cd frontend && npm install",
    "lint": "cd frontend && npm run lint",
    "lint:fix": "cd frontend && npm run lint:fix",
    "format": "cd frontend && npm run format",
    "test": "cd frontend && npm run test",
    "test:ui": "cd frontend && npm run test:ui",
    "clean": "rm -rf frontend/dist frontend/node_modules backend/__pycache__ backend/uploads/* backend/outputs/*",
    "vercel-build": "npm run build:vercel",
    "deploy": "vercel --prod",
    "deploy:preview": "vercel",
    "deploy:render": "echo 'Deploy using render.yaml configuration'",
    "test:deployment": "npm run build && npm run start"
  },
  "keywords": [
    "veroctaai",
    "financial-intelligence",
    "ai",
    "spendscore",
    "flask",
    "react",
    "typescript"
  ],
  "author": "VeroctaAI Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^7.6.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0",
    "python": ">=3.11.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/verocta-ai/verocta-unified.git"
  },
  "bugs": {
    "url": "https://github.com/verocta-ai/verocta-unified/issues"
  },
  "homepage": "https://verocta.ai"
}
